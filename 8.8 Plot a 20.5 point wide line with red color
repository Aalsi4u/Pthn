//Write a program to plot a 20.5 point wide line with red color.

import sys
import matplotlib
matplotlib.use('Agg')
import matplotlib.pyplot as plt
import numpy as np
ypoints = np.array([3, 8, 1, 10])
plt.plot(ypoints, linewidth = '20.5',color='r')
plt.show()
plt.savefig(sys.stdout.buffer)
sys.stdout.flush()
